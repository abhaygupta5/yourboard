<div class="row">

            <!-- Blog Entries Column -->
            <div class="col-md-8">

                <h1 class="page-header">
                    Log In
                </h1>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email, required: false, autofocus: true,class: "form-control" %>
    </div>
    <div class="form-group">
    <%= f.label :password, class: "labelclass" %>
    <%= f.password_field :password, required: false,class: "form-control" %>
    </div>
    <%= f.label :remember_me , class: "remember" %>
    <%= f.check_box :remember_me,class: "remember1", as: :boolean if devise_mapping.rememberable? %>
  </div>
  <br>
  <div class="form-actions">
    <%= f.button :submit, "Log in", class: "btn btn-primary" %>
  </div>
<% end %>
<br><br>
<%= render "devise/shared/links" %>

<hr>
</div>

<%= render 'partials/search' %>
<%= render 'partials/categories' %>
<%= render 'partials/widget' %>
</div>